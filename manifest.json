{
    "manifest_version": 3,
    "name": "My Puppeteer Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension using Puppeteer.",
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png"
        }
    }
}
